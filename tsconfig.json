{
  "extends": "plasmo/templates/tsconfig.base",
  "exclude": [
    "node_modules"
  ],
  "include": [
    ".plasmo/index.d.ts",
    "src/**/*.ts",
    "src/**/*.tsx",
    "cube-dist/**.ts"
  ],
  "compilerOptions": {
    "paths": {
      "~*": [
        "./*"
      ],
      "@": [
        "./src/*"
      ]
    },
    "baseUrl": "src/"
  }
}
